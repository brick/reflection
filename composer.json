{
    "name": "brick/reflection",
    "description": "Low-level tools to extend PHP reflection capabilities",
    "type": "library",
    "keywords": [
        "Brick",
        "Reflection"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "brick/varexporter": "^0.3.7 || ^0.4.0 || ^0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "vimeo/psalm": "6.13.1"
    },
    "autoload": {
        "psr-4": {
            "Brick\\Reflection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brick\\Reflection\\Tests\\": "tests/"
        }
    }
}
