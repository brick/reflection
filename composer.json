{
    "name": "brick/reflection",
    "description": "Low-level tools to extend PHP reflection capabilities",
    "type": "library",
    "keywords": [
        "Brick",
        "Reflection"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "doctrine/annotations": "^1.10.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Brick\\Reflection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brick\\Reflection\\Tests\\": "tests/"
        }
    }
}
